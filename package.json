{
  "name": "ace-redis-cache",
  "version": "0.5.0",
  "description": "Smart Redis-powered caching with WordPress Block API support and configurable exclusions",
  "main": "ace-redis-cache.php",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "build:css": "sass assets/src/css/admin.scss assets/dist/admin-styles.min.css --style=compressed --source-map",
    "build:js": "webpack --mode=production --entry=./assets/src/js/admin.js --output-path=./assets/dist/",
    "watch:css": "sass assets/src/css/admin.scss assets/dist/admin-styles.min.css --style=compressed --source-map --watch",
    "watch:js": "webpack --mode=development --watch --entry=./assets/src/js/admin.js --output-path=./assets/dist/",
    "lint:js": "eslint assets/src/js --ext .js",
    "lint:css": "stylelint assets/src/css/**/*.scss",
    "lint": "npm run lint:js && npm run lint:css",
    "test": "phpunit",
    "clean": "rm -rf assets/dist/*"
  },
  "keywords": [
    "wordpress",
    "redis",
    "cache",
    "performance",
    "optimization"
  ],
  "author": "Ace Media",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.52.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
